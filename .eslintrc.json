{
	"extends": "airbnb",
	"env": {
		"browser": true,
		"commonjs": true,
		"es6": true,
		"node": true,
		"jest": true
	},
	"rules": {
		"react/jsx-filename-extension": [
			1,
			{
				"extensions": [".js", ".jsx"]
			}
		],
		"linebreak-style": 0,
		"arrow-body-style": 0,
		"comma-dangle": 0,
		"space-in-parens": 0,
		"array-bracket-spacing": 0,
		"semi": 0,
		"no-return-assign": 0,
		"import/no-extraneous-dependencies": [
			"error",
			{
				"devDependencies": true
			}
		],
		"react/forbid-prop-types": [false],
		"no-underscore-dangle": "off"
	},
	"parser": "babel-eslint"
}
